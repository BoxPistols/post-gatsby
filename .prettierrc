{
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": false,
	"semi": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"arrowParens": "always",
	"endOfLine": "lf",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"vueIndentScriptAndStyle": false
}
